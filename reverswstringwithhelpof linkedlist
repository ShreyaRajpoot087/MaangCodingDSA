class Solution {
public:
    void reverseString(vector<char>& A) {
        /*
        stack<char> st;
        for(int i=0;i<A.size();i++)
        {
            //pushing every char inside stack
            st.push(A[i]);

        }
        // pop every char
        for(int i =0;i<A.size();i++)
        {
            A[i]= st.top();
            st.pop();

    }
    */
     int s=0;
     int e = A.size()-1;
      while(s<=e){
        swap(A[s],A[e]);
        s++;
        e--;
      }
    }
};